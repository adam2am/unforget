{
  "manifest_version": 2,
  "name": "Prompt Templates",
  "version": "1.0",
  "description": "Display Prompt Templates from Chatbot UI on chat.openai.com",
  "permissions": ["tabs", "https://chat.openai.com/*"],
  "content_scripts": [
    {
      "matches": ["https://chat.openai.com/*"],
      "js": ["content_script.js"],
      "css": ["styles.css"]
    }
  ]
}